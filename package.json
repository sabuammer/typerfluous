{
  "name": "typerfluous",
  "description": "A simple typing tester with multiplayer capabilities",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper export && rm -rf ./public/* && mv ./__sapper__/export/* ./public",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@n0n3br/svelte-persist-store": "^1.0.2",
    "compression": "^1.7.1",
    "crypto-js": "^4.0.0",
    "firebase": "^7.16.1",
    "identicon.js": "^2.3.3",
    "polka": "next",
    "random-words": "^1.1.1",
    "sirv": "^0.4.0",
    "svelte-forms-lib": "^1.1.1",
    "svelte-notifications": "^0.9.9",
    "svelte-persistent-store": "^0.1.3",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@beyonk/svelte-notifications": "^2.0.3",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.22.1",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.16",
    "svelte": "^3.24.0"
  }
}
